<!DOCTYPE html>
<html lang="en">
#parse("content/base/back/common/head.vm")
<body style='overflow:scroll;overflow-x:hidden'>
	<div class="container bs-docs-container" style="width:100%;">
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-heading">编辑</div>
				<div class="panel-body">
					<form class="form-horizontal" role="form" id="dailogForm" action="$!{basePath}/dictinfo/back/dictinfo/doEdit.html" method="POST">
						 <input type="hidden" id="btn_sub" class="btn_sub" />
						 <input type="hidden" value="$!{dictinfo.id}" name="id" id="id" />
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">字典类型ID</label>
												    <div class="col-sm-8">
												       <select name="typeid" id="typeid"  value="$!{dictinfo.typeid}" class="form-control" >
												       	#if($!{pageInfos})
											                #foreach($!{info} in $!{pageInfos})
											               	 <option value="$!{info.id}" #if($!{info.id} eq $!{dictinfo.typeid})selected#end>$!{info.typename}</option>
												             #end
											    	 	#end
												      
												      </select>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">字典代码</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{dictinfo.dictcode}" name="dictcode" id="dictcode" class="form-control"/>
												    </div>
												</div>
												<div class="form-group mno">
												    <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">字典名称</label>
												    <div class="col-sm-8">
												      <input type="text" value="$!{dictinfo.dictname}" name="dictname" id="dictname" class="form-control"/>
												    </div>
												</div>
												
									<div class="form-group mno">
											<div class="col-sm-offset-1 col-sm-6">
												<button type="button" class="btn btn-default" id="formReturn" data-dismiss="modal" onclick="doUrl('$!{basePath}/dictinfo/back/dictinfo/list.html')">返回</button>
	        									<button type="button" class="btn btn-primary" id="formSubmit">提交</button>
        									 </div>
        							</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
<script type="text/javascript" src="$!{basePath}/plug-in/js/Validform_v5.3.2.js"></script> 
<script type="text/javascript" src="$!{basePath}/plug-in/js/forminit.p3.js"></script>  